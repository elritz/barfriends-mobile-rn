{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "#/*": ["*"],
      "#/src": ["./src"],
      "#/app/*": ["./app/*"],
      "#/types/*": ["./types/*"],
      "#/assets/*": ["assets/*"],
      "#/graphql/*": ["./graphql/*"],
      "#/reactive": ["./src/state/reactive/index.tsx"],
      "#/gluestack": ["./gluestack-ui.config"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts"
  ]
}
