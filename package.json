  "name": "barfriends",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint ",
    "rc-start": "bun pm cache rm",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && k",
    "clean-pods": "pod repo update",
    "clean-start": "bun run clean && bun run rc-start",
    "fresh-install": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && bun pm cache rm && bun install",
    "fresh-start": "bun run fresh-install && bun run rc-start",
    "clear-bundler-cache": "rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && rm -rf $TMPDIR/hermes-*",
    "clear-watchmen": "watchman watch-del-all",
    "dev": "NODE_ENV=development bunx expo start",
    "dev-build": "NODE_ENV=development bunx expo start --dev-client",
    "dev-production": "NODE_ENV=production bunx expo start --no-dev --minify",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "verify": "bun run tsc && bun lint --quiet && bun format",
    "format": "prettier --write ./ ",
    "eject": "expo eject",
    "expo:login": "eas login -u $EXPO_USERNAME -p $EXPO_PASSWORD",
    "gen-s": "graphql-codegen --config codegen-graphql-sub.yml",
    "gen": "graphql-codegen --config codegen-graphql.ts",
    "gents": "bunx expo customize tsconfig.json",
    "extract": "lingui extract",
    "compile": "lingui compile",
    "postversion": "react-native-version",
    "prebuild:ios:development:clean": "NODE_ENV=development expo prebuild --platform ios --clean",
    "prebuild:ios:development": "NODE_ENV=development expo prebuild --platform ios",
    "prebuild:ios:production": "NODE_ENV=production expo prebuild --platform ios --clean",
    "eas:device:create": "eas device:create",
    "eas:build:ios:test:auto:submit": "NODE_ENV=test eas build --platform ios --profile test-ios --auto-submit-with-profile=production --no-wait",
    "eas:build:ios:devclient": "NODE_ENV=development eas build --platform ios --profile development-client --no-wait",
    "eas:build:ios:dev": "NODE_ENV=development eas build --platform ios --profile development --no-wait",
    "eas:build:ios:preview": "NODE_ENV=production eas build --platform ios --profile preview --no-wait",
    "eas:build:ios:preview:auto:submit": "NODE_ENV=production eas build --platform ios --profile preview --auto-submit-with-profile=preview --no-wait",
    "eas:build:ios:local:simulator:devclient": "NODE_ENV=development eas build --local --platform ios --profile development-simulator --no-wait",
    "eas:build:ios:local:device:devclient": "NODE_ENV=development eas build --local --platform ios --profile development-client --no-wait",
    "eas:build:ios:local:dev": "NODE_ENV=development eas build --local --platform ios --profile development --no-wait",
    "eas:build:ios:local:preview": "NODE_ENV=development eas build --local --platform ios --profile preview --no-wait",
    "eas:build:ios:local:production": "NODE_ENV=production eas build --local --platform ios --no-wait --auto-submit",
    "eas:build:android:simulator": "NODE_ENV=development eas build --platform android --profile development --no-wait",
    "eas:build:android:preview": "NODE_ENV=production eas build --platform android --profile preview --no-wait",
    "eas:build:android:production": "NODE_ENV=production eas build --platform android --no-wait --auto-submit",
    "eas:build:android:local:simulator:devclient": "NODE_ENV=development eas build --local --platform android --profile development-simulator --no-wait",
    "eas:build:android:local:device:devclient": "NODE_ENV=development eas build --local --platform android --profile development-client --no-wait",
    "eas:build:android:local:dev": "NODE_ENV=development eas build --local --platform android --profile development --no-wait",
    "eas:build:android:local:preview": "NODE_ENV=development eas build --local --platform android --profile preview --no-wait",
    "eas:build:android:local:production": "NODE_ENV=production eas build --local --platform android --no-wait --auto-submit",
    "eas:submit:ios": "NODE_ENV=production eas submit --platform ios --latest",
    "eas:submit:ios:preview:latest": "NODE_ENV=production eas submit --platform ios --latest --profile preview",
    "eas:submit:android": "NODE_ENV=production eas submit --platform android --latest",
    "postinstall": "patch-package"
  },
