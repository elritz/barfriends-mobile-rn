{
  "cli": {
    "version": ">= 12.6.2",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "android": {
        "env": {
          "PLATFORM": "android"
        }
      },
      "ios": {
        "image": "latest",
        "env": {
          "PLATFORM": "ios"
        }
      }
    },
    "development-simulator": {
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      },
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development-client": {
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      },
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "development": {
      "extends": "base",
      "env": {
        "NODE_ENV": "development"
      },
      "channel": "development",
      "distribution": "internal"
    },
    "preview": {
      "extends": "base",
      "channel": "preview",
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EAS_LOCAL_BUILD_ARTIFACTS_DIR": "./artifacts/preview/"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
